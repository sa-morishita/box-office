{
  "extends": [
    "eslint:recommended",
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "react-hooks"
  ],
  "env": {
    "browser": true
  },
  "parser": "babel-eslint",
  "rules": {
    "no-unused-vars":["warn"],
    "arrow-body-style":["warn"],
    "no-plusplus": [
      0
    ],
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "react/prop-types": [
      0
    ],
    "react/jsx-props-no-spreading": [
      0
    ],
    "import/prefer-default-export": [
      0
    ],
    "react/no-array-index-key": [
      0
    ],
    "react/no-danger": [
      0
    ],
    "no-console": [
      0
    ],
    "react-hooks/rules-of-hooks": "warn",
    "react-hooks/exhaustive-deps": "warn"
  }
}